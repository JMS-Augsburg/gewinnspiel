<div class="table-responsive">
	<table class="table table-striped table-hover">
		<thead>
			<tr>
				<td>#</td>
				<td>Name</td>
				<td>E-Mail</td>
				<td>Zeitstempel</td>
				<td>randomVal</td>
			</tr>
		</thead>
		<tbody>
			{{#each users}}
				<tr>
					<td>{{ this.id }}</td>
					<td>{{ this.name }}</td>
					<td>{{ this.email }}</td>
					<td>{{ this.timestamp }}</td>
					<td>{{ this.randomVal }}</td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>
